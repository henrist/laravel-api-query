{
    "name": "henrist/laravel-api-query",
    "description": "Package making listing of API-endpoint easier",
    "license": "MIT",
    "authors": [
        {
            "name": "Henrik Steen",
            "email": "henrist@henrist.net"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/database": ">=11",
        "illuminate/http": ">=11",
        "illuminate/pagination": ">=11",
        "illuminate/support": ">=11"
    },
    "require-dev": {
        "laravel/pint": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Henrist\\LaravelApiQuery\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Henrist\\LaravelApiQuery\\LaravelApiQueryServiceProvider"
            ],
            "aliases": {
                "ApiQuery": "Henrist\\LaravelApiQuery\\ApiQuery"
            }
        }
    }
}
